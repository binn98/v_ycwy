/* pages/rules/rules.wxss */
.ycwy-container {
  background: #fff;
  overflow: hidden;
  padding-bottom: 100rpx;
}
.ycwy-container .wux-accordion__hd {
  background: #fff;
  color: #181818;
  font-size: 28rpx;
}
.ycwy-container .wux-accordion--current > .wux-accordion__bd {
  background-color: #F7F7F7 !important;
  margin: 30rpx;
  padding: 20rpx 30rpx;
  border-radius: 15rpx;
  color: #999;
  font-size: 26rpx;
}



