<!--pages/index/index.wxml-->
<view class="ycwy-container">
    <view class="information">
  <!-- 个人信息 -->
  <view class="per_details" style="background-image: url(data:image/jpg;base64,)" >
    <view class="per_header">
      <image class="per_img" src="{{info.avatar_url}}" mode="widthFix"></image>
    </view>
    <view class="per_info" wx:if="{{isLogin}}">
      {{info.nick_name}}
      <view class="id">
        账号:{{info.mobile}}
      </view>
    </view>
    <view class="per_info" wx:if="{{isLogin==false}}">
      <button  class="_btn" open-type="getPhoneNumber" bindgetphonenumber="getInfoCallback" wx:if="{{ !isLogin }}">注册/登录</button>
    </view>
    <view class="Rec_rules" bindtap="gorules">
      <view class="rule">
        回收规则
      </view>
    </view>
  </view>
  <!-- 优惠券 -->
  <view class="coupon">
   <view class="c_amount" bindtap="dd">
    旧衣回收优惠券金额
   </view> 
   <view class="{{totals>0? 'c_frames' :'c_frame'}}">
     <view class="c_rmb">
       {{ total.usable_money || 0 }}
     </view>
     元/{{ total.usable_num ||0}}张
   </view>
   <view class="check" bindtap="goCoupon" wx:if="{{isLogin}}">
     查看明细 <text class="iconfont icon-jiantou-right"  ></text>
   </view>
   <view class="xiaoxi" wx:if="{{total.new_create_time}}">
     <text class="iconfont icon-weibiaoti1"></text>最新:{{total.new_create_time}} 划入通兑优惠券:{{total.new_money}}元
   </view>
   <view class="xiaoxi" wx:else>
     
   </view>
  </view>
    </view>
    <view class="tabWrap {{scrollTop>200? 'tabWraps':''}}">
    	<wux-tabs defaultCurrent="{{ tab }}" bind:change="tabChange" class="tabs" data-tab="tab">
      		<wux-tab key="0">可回收清单</wux-tab>
      		<wux-tab key="10" title="已回收清单"></wux-tab>
    	</wux-tabs>
    	<view class="tabs_" wx:if="{{tab!=10}}">
      		<text class="tabs_q {{ative==false? 'ative':''}}"  bindtap="getAllList">全部</text>
      		<text class="tabs_s {{ative!=false? 'ative':''}}" bindtap="filterList"> {{tabs==''? '筛选':tabs}} <text    class="iconfont {{show==false? 'icon-RectangleCopy':'icon-RectangleCopy1'}}"  ></text></text>
      </view>
      <view class="tabs_h {{show==true? 'show':''}}">
        <text class="{{tab2==30? 'clor':''}}" bindtap="getfilterList" data-day="30" data-value="30天内">30天内</text>
        <text class="{{tab2==60? 'clor':''}}" bindtap="getfilterList" data-day="60" data-value="60天内">60天内</text>
        <text class="{{tab2==90? 'clor':''}}" bindtap="getfilterList" data-day="90" data-value="90天内">90天内</text>
        <text class="{{tab2==100? 'clor':''}}" bindtap="getfilterList" data-day="100" data-value="90天以上">90天以上</text>
        </view>
	</view>

    <view class="list" wx:if="{{ list && !loading && list.length > 0 }}">
      <view class="list_box" wx:for="{{ list }}" wx:for-item="item" wx:key="goods_id">
        <view class="title">
          <text bindtap="forList">订单编号：{{ item.order_no }}</text>
        </view>
        <view class="inf">
         <view class="img">
            <image mode="widthFix" src="{{ item.image }}"></image>
         </view>
          <view class="text">
            <text class="bold wux-ellipsis--l2">{{ item.name }}</text>
            <text class="block_">{{item.sku_properties_name[1].v }};{{item.sku_properties_name[0].v }}</text>
            <view class="price_">
              <view class="left">
                <text class="price">商品售价￥{{ item.price }}</text>
                <text class="huisoujia">回收价 ￥ <text class="rmb">{{item.recovery_price}}</text></text>
              </view>
              <view wx:if="{{ tab == 0 && item.logistic_status < 10 }}" class="{{ active[index] == true? 'btns':'btn'}}" bindtap="Checked" data-id="{{ item.goods_id }}" data-name="{{ item.name }}" data-index="{{index}}" data-num="{{ item.recovery_price }}" data-ep="{{ item.express_price }}" data-status="{{ item.logistic_status }}" data-rid="{{ item.rec_order_id }}">{{active[index] == true? '取消':'回收'}}
              </view>
              <view class="reds" wx:if="{{ tab == 10}}">回收成功</view>
              <view data-id="{{ item.rec_order_id }}" bindtap="godetilt" class="red" wx:if="{{ tab == 0 && item.logistic_status == 10 }}">等待收件>></view>
            </view>  
          </view>
        </view>
        <view class="rushoutime">
          <text>
           <text>入手时间：{{ item.success_time_txt }}</text>
         </text>
         <text style="color: #FF5E62;" wx:if="{{ timeList.length > 0 &&  tab == 0}}">已使用：{{ timeList[index].d + '天 ' + timeList[index].h + ':' + timeList[index].m + ':' + timeList[index].s}}</text>
         <!-- <text style="color: #FF5E62;" wx:if="{{ tab == 10 }}">已到账优惠券 {{ item.recovery_price * 1 + item.express_price * 1 }}元</text> -->
         <text wx:if="{{ tab == 10 }}"></text>
         <text class="delid" wx:if="{{tab == 10}}" data-id="{{item.rec_order_id}}" bindtap="del">删除订单</text>
       </view>
      </view>
    </view>

    <view class="list list_no tac" wx:elif="{{ !loading && isLogin}}">
      <image mode="widthFix" src="../../assets/img/order_no.png"></image>
      <text class="tips block tac" wx:if="{{tab2 > 0 }}">{{  tab2 == 100 ? '90天以上' : tab2 + '天内'  + '暂无回收衣物清单'}}</text>
      <text bindtap="goOther" class="tips block tac" wx:else="{{tab2<0 }}">
        暂时没有衣物可回收喔,去 
          <text style="color: #FF5E62; display: inline;">看看新衣吧~</text>       
      </text>
    </view>
    <view class="denglu" wx:if="{{ !isLogin }}">
      <button class="_btn_bttom" open-type="getPhoneNumber" bindgetphonenumber="getInfoCallback" >微信授权登录</button>
      <view class="wenan">登录衣橱无忧商城账户查看</view>
    </view>
    <view class="bot_Submit" wx:if="{{tab==0&&list.length>0}}">
      <view class="shuoming" >
       <view class="shuoming_" >
        <view class="shuoming_gold">
          旧衣回收优惠券金额
         </view>
         <view class="shuoming_gold_count">
          （由{{goods_id.length}}张优惠券组成）
         </view>
       </view>
       <view class="rmb">
        ￥{{Rmb}}
       </view>
      </view>
      <view class="submit {{Rmb>0==true? 'submit_':''}}" data-moeny="{{Rmb}}" bindtap="goTo">
        提交回收
      </view>
    </view>
    <view class="mask" wx:if="{{show}}"></view> 
</view>