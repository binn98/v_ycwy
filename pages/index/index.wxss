  border-radius: 25rpx;
  background-color: #f5f5f5;
}
.tabWrap .tabs_ .tabs_q {
  margin-bottom: 16rpx;
}
.tabs_s {
  z-index: 901;
}
.tabs_h {
  z-index: 901;
  height: 0rpx;
  transition: height 0.5s;
  overflow: hidden;
  background-color: #f7f7f7;
  position: relative;
}
.tabs_h > text {
  display: inline-block;
  line-height: 104rpx;
  width: 25%;
  text-align: center;
  font-size: 26rpx;
}
