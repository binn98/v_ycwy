.tabWrap {
  width: 100%;
  /* height: 200rpx; */
  z-index: 999;
  position: relative;
}
.tabWraps {
  position: fixed;
  top: 0;
  left: 0;
}
.tabWrap .tabs_ {
  display: flex;
  justify-content: space-between;
  padding: 0 60rpx;
  padding-top: 28rpx;
  font-size: 26rpx;
  background-color: #fff;
}
.tabWrap .tabs_ > text {
  display: inline-block;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
