/* pages/index/index.wxss */
.ycwy-container {
  background: #F6F6F6;
  overflow: hidden;
  padding-bottom: 100rpx;
}



.tabWrap {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 12;
  width: 100%;
  height: 200rpx;
}
.tabs .wux-tabs {
  height: 100rpx;
  line-height: 100rpx;
}
.tabs .wux-tabs__tab {
  color: #181818;
}
.tabs .wux-tabs__tab--balanced.wux-tabs__tab--current {
  color: #FF7C64;
}
.tabs .wux-tabs__tab--balanced .wux-tabs__tab-bar {
  background: #FF7C64;
  
}
.secTabs .wux-tabs {
  height: 100rpx;
  line-height: 100rpx;
  background: #F6F6F6;
  /* margin-bottom: 20rpx; */
}
.secTabs .wux-tabs__tab {
  color: #181818;
}
.secTabs .wux-tabs__tab--balanced.wux-tabs__tab--current {
  color: #FF7C64;
}
.secTabs .wux-tabs__tab--balanced .wux-tabs__tab-bar {
  background: none;
  /* background-color: #FF7C64;
  margin-top: -20rpx; */
}
