<!--pages/detail/detail.wxml-->
<view class="ycwy-container">
  <view class="info tac pdt_45 pdb_60">
    <text class="iconfont icon-dagou- " wx:if="{{ detail.recovery_type == 10 }}"></text>
    <view wx:else><image mode="widthFix" src="../../assets/img/success.png" style="width: 254rpx;"></image></view>
    <text class="msg block" wx:if="{{ detail.recovery_type == 10 }}">快递信息提交成功</text>
    <text class="msg block" wx:elif="{{ detail.recovery_type == 20 && detail.status == 10 }}">回收中</text>
    <text class="msg block" wx:elif="{{ detail.recovery_type == 20 && detail.status == 20 }}">回收成功</text>
    <text class="tips block" wx:if="{{ !detail.logistic_num }}">等待快递员上门回收，请注意接听电话！</text>
    <view class="detail">
      <text>下单时间： {{ detail.create_time_txt }}</text>
      <text wx:if="{{ detail.recovery_type == 10 }}">上门时间： {{ detail.time_txt }}</text>
      <text>快递公司： {{ detail.logistic_name }}</text>
      <view class="tal" wx:if="{{ detail.logistic_num }}">快递单号： {{ detail.logistic_num }}</view>
      <view class="tal" wx:else style="color: #181818;">快递单号：
        <wux-input label="" value="{{ num }}" data-name="num" placeholder="输入快递单号" controlled type="number" bind:change="formChange" bind:blur="formChange" />
        <view class="btn" bindtap="postNum">提交</view>
      </view>
      <text>客服电话： {{ detail.consumer_hotline }}</text>
      <view class="fixed" wx:if="{{ detail.logistic_num }}" bindtap="goTo" data-name="{{ detail.logistic_name }}" data-code="{{ detail.logistic_code }}" data-num="{{ detail.logistic_num }}">查看物流信息</view>
    </view>
  </view>

  <view class="ticket" style="background-image: url(data:image/png;base64,)">
    <view class="title flex flex_mj flex_cc">
      <text>等待平台回收确认，即可激活优惠券</text>
      <view class="btn" bindtap="goTo2">优惠券账户</view>
    </view>
    <view class="wrap flex flex_cc">
      <view class="price mgl_40">￥<text>{{ price + detail.express_price * 1 }}</text></view>
      <view class="all mgl_30">
        <text style="font-size: 28rpx;">回收后可累计获得通兑优惠券</text>
        <text>（ 回收价:￥{{ price }}/件 + 快递费:￥{{ detail.express_price * 1 }} ）</text>
      </view>
    </view>
  </view>
  <view class="ttips">通兑优惠券是一种无门槛兑换优惠券，适用于衣橱无忧商城购买全品类衣物进行抵扣。</view>

<view class="address address2" wx:if="{{ detail.recovery_type == 10 }}">
    <view class="wrap mgt_30 flex flex_cc">
      <image mode="widthFix" class="mgl_30" style="width: 60rpx;" src="data:image/svg+xml;base64,"></image>
      <!-- <text class="iconfont icon-jijiandizhi-" style="color: #538CFF"></text> -->
      <view class="info flex flex_dt">
        <view class="fwb">{{ detail.sender_name }} <text class="fwn mgl_30">{{ detail.sender_mobile }}</text></view>
        <view class="light mgt_10">{{ detail.sender_province + detail.sender_city + detail.sender_area + detail.sender_address }}</view>
      </view>
      <!-- <text class="iconfont icon-bianji-" style="color: #181818; font-size: 40rpx;"></text> -->
    </view>
  </view>

  <view class="address2">
    <view class="wrap mgt_30 flex flex_cc">
    <image mode="widthFix" class="mgl_30" style="width: 60rpx;" src="data:image/svg+xml;base64,"></image>
      <!-- <text class="iconfont icon-dizhi-"></text> -->
      <view class="info flex flex_dt">
        <view class="fwb">{{ detail.receiver_name }} <text class="fwn mgl_30">{{ detail.receiver_mobile }}</text></view>
        <view class="light mgt_10">{{ detail.receiver_province + detail.receiver_city + detail.receiver_area + detail.receiver_address }}</view>
      </view>
    </view>
  </view>
</view>