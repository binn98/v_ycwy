/* pages/detail/detail.wxss */
.ycwy-container {
  background: #F6F6F6;
  overflow: hidden;
  padding-bottom: 100rpx;
}



.info {
  margin: 40rpx 30rpx 0;
  background: #FFFFFF;
  border-radius: 10rpx;
}
.info .icon-dagou- {
  color: #04C160;
  font-size: 100rpx;
}
.info .msg {
  color: #04C160;
  font-size: 36rpx;
  padding: 20rpx 0;
}
.info .tips {
  color: #181818;
  font-size: 28rpx;
}
.info .detail {
  position: relative;
  padding-top: 30rpx;
  color: #999999;
  font-size: 24rpx;
}
.info .detail text,
.info .detail .tal {
  padding: 15rpx 55rpx;
  display: block;
  text-align: left;
}
.info .detail wux-input {
  display: inline-block;
  width: 300rpx;
  padding: 10rpx;
  border: 1rpx solid #D4D3D3;
}
.info .detail .wux-input__item {
  font-size: 24rpx;
  line-height: 58rpx;
}
.info .detail .btn {
  display: inline-block;
  width: 100rpx;
  height: 58rpx;
  margin-left: 20rpx;
  background: #FF7C64;
  border-radius: 5rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  line-height: 58rpx;
  text-align: center;
  vertical-align: top;
}
.info .detail .fixed {
  position: absolute;
  bottom: 0;
  right: 40rpx;
  z-index: 12;
  width: 180rpx;
  height: 60rpx;
  background: #FF7C64;
  border-radius: 5rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  line-height: 60rpx;
  text-align: center;
}



.ticket {
  height: 240rpx;
  margin: 20rpx 30rpx 0;
  background-size: 100% auto;
  background-position: center center;
}
.ticket .title {
  height: 72rpx;
  padding: 0 30rpx;
  color: #FFFFFF;
  font-size: 26rpx;
}
.ticket .title .btn {
  padding: 8rpx 10rpx;
  border: 1rpx solid #FFFFFF;
  border-radius: 8rpx;
}
.ticket .wrap {
  height: 170rpx;
}
.ticket .wrap .price {
  color: #FDB9A0;
  font-size: 36rpx;
}
.ticket .price text {
  font-size: 80rpx;
}
.ticket .wrap .all {
  color: #181818;
  font-size: 24rpx;
}
.ticket .all text {
  display: block;
}
.ttips {
  padding: 20rpx 40rpx;
  color: #999999;
  font-size: 24rpx;
}



.address2 {
  margin: 20rpx 30rpx 0;
}
.address2>text {
  color: #262626;
  font-size: 26rpx;
}
.address2 .wrap {
  width: 100%;
  height: 150rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
}
.address2 .iconfont {
  color: #E3805B;
  font-size: 60rpx;
  margin: 0 30rpx;
}
.address2 .info {
  margin-top: 0;
  width: 460rpx;
  color: #262626;
  font-size: 30rpx;
}
.address2 .info text {
  font-size: 26rpx;
}
.address2 .info .light {
  color: #999999;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}