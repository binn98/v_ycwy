/* pages/user/user.wxss */
.ycwy-container {
  background: #F6F6F6;
  overflow: hidden;
  padding-bottom: 100rpx;
}



.header {
  width: 100%;
  height: 280rpx;
  background: #FF7C64;
}
.header>view {
  padding: 0 30rpx;
}
.header .hdimg {
  width: 120rpx;
  height: 120rpx;
  background: #FFFFFF;
  border-radius: 50%;
  overflow: hidden;
}
.header .hdimg image {
  width: 100%;
}
.header .text {
  color: #FFFFFF;
  font-size: 40rpx;
  font-weight: 700;
}
.header .text text {
  font-size: 28rpx;
  font-weight: 400;
}
.header .ques {
  width: 125rpx;
  height: 40rpx;
  background: #FFFFFF;
  border: 1rpx solid #FFFFFF;
  border-radius: 20rpx;
  color: #232323;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
}



.total {
  height: 280rpx;
  margin: -60rpx 30rpx 0;
  background: #FFFFFF;
  border-radius: 10rpx;
}
.total .title {
  display: block;
  color: #212121;
  font-size: 24rpx;
}
.total .all {
  display: block;
  color: #9E4E3E;
  font-size: 24rpx;
}
.total .all text {
  font-size: 90rpx;
}
.total .new {
  display: inline-block;
  padding: 4rpx 8rpx;
  border: 1rpx solid #E3805B;
  border-radius: 20rpx;
  color: #E3805B;
  font-size: 24rpx;
}



.tabs .wux-tabs {
  height: 100rpx;
  line-height: 100rpx;
}
.tabs .wux-tabs__tab {
  color: #181818;
}
.tabs .wux-tabs__tab--balanced.wux-tabs__tab--current {
  color: #FF7C64;
}
.tabs .wux-tabs__tab--balanced .wux-tabs__tab-bar {
  background: #FF7C64;
}
.list>.wrap {
  padding-bottom: 30rpx; 
}
.ticket {
  position: relative;
  height: 230rpx;
  margin: 20rpx 30rpx 0;
  background-size: 100% auto;
  background-position: center center;
}
.ticket.disabled .wrap .price {
  color: #999999;
}
.ticket.disabled .wrap .tt {
  color: #999999 !important;
}
.ticket .title {
  height: 72rpx;
  padding: 0 30rpx;
  color: #FFFFFF;
  font-size: 22rpx;
}
.ticket .title .btn {
  padding: 8rpx 10rpx;
  border: 1rpx solid #FFFFFF;
  border-radius: 8rpx;
}
.ticket .wrap {
  height: 170rpx;
}
.ticket .wrap .price {
  color: #FDB9A0;
  font-size: 36rpx;
}
.ticket .price text {
  font-size: 60rpx;
}
.ticket .wrap .all {
  color: #999999;
  font-size: 22rpx;
}
.ticket .all text {
  display: block;
}
.ticket .fixed {
  position: absolute;
  top: 12rpx;
  right: 0;
  z-index: 12;
}
.ticket .fixed image {
  width: 93rpx;
}
.list_no {
  min-height: 600rpx;
  padding-bottom: 30rpx;
  background: #FFFFFF;
}
.list_no image {
  width: 286rpx;
  padding-top: 50rpx;
}
.list_no .tips {
  color: #181818;
  font-size: 28rpx;
}
.list_no .wux-divider__text {
  color: #999999;
  font-size: 24rpx;
}
.list_no .bigimg {
  width: 670rpx;
  padding-top: 15rpx;
}



.nav {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 12;
  width: 100%;
  height: 100rpx;
  background: #FFFFFF;
}
.nav navigator {
  width: 50%;
  height: 100%;
  color: #181818;
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: center;
}
.nav .iconfont {
  margin-right: 12rpx;
}
.nav .cur {
  background: #FF7C64;
  color: #FFFFFF;
}