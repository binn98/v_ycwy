<!--pages/user/user.wxml-->
<view class="ycwy-container">
  <view class="header">
    <view class="flex flex_mj">
      <view class="flex flex_cc" style="height: 210rpx;">
        <view class="hdimg"><image mode="widthFix" src="{{ info.avatar_url }}"></image></view>
        <view class="text flex flex_dt mgl_20" wx:if="{{ isLogin }}">
          {{ info.nick_name }}<text>衣橱无忧账户：{{ info.mobile }}</text>
        </view>
        <button class="text flex flex_dt btn_text" open-type="getPhoneNumber" bindgetphonenumber="getInfoCallback" wx:else style="background: none; border: none; padding-left: 20rpx;">登录/注册</button>
      </view>
      <view class="ques mgt_45" bindtap="goTo">常见问题</view>
    </view>
  </view>

  <view class="total tac">
    <text class="title pdt_45">优惠券账户额度可用</text>
    <block wx:if="{{ total.usable_money }}">
    <text class="all">￥<text>{{ total.usable_money }}</text>/{{ total.usable_num }}张</text>
    <text class="new">最新：{{ total.new_create_time }} 划入通兑优惠券:￥{{ total.new_money }}</text>
    </block>
    <text class="block tac pdt_80" wx:else>暂无数据</text>
  </view>

  <view class="list mgt_20">
    <wux-tabs defaultCurrent="{{ tab }}" bind:change="tabChange" class="tabs" data-tab="tab">
      <wux-tab key="10" title="未使用"></wux-tab>
      <wux-tab key="20" title="已使用"></wux-tab>
      <wux-tab key="30" title="已失效"></wux-tab>
    </wux-tabs>

    <view class="wrap mgt_30" wx:if="{{ list.length > 0 && !loading }}">
      <view class="ticket {{ tab != 10 ? 'disabled' : '' }}" style="background-image: url({{ tab == 10 ? 'data:image/png;base64,' : 'data:image/png;base64,' }})" wx:for="{{ list }}" wx:key="id" wx:for-item="item">
        <view class="wrap flex flex_mj flex_cc">
          <view class="price mgl_40 tar">￥<text>{{ item.recovery_money * 1 + item.express_money * 1 }}</text><text class="block" style="font-size: 24rpx;">通兑优惠券</text></view>
          <view class="all mgr_20">
            <text class="tt" style="color: #181818; font-size: 28rpx;">划入：{{ item.create_time_txt }}</text>
            <text>来源订单：{{ item.order_no }}</text>
            <text>有效期：{{ item.effect_sday }} 至 {{ item.effect_eday }}</text>
          </view>
        </view>
        <view class="title flex flex_mj flex_cc">
          <text>(回收价:￥{{ item.recovery_money * 1 }} + 快递费: ￥{{ item.express_money * 1 }})</text>
          <text>优惠券编号：{{ item.yz_code }}</text>
        </view>
        <view class="fixed" wx:if="{{ tab == 20 }}"><image mode="widthFix" src="../../assets/img/ico-used.png"></image></view>
        <view class="fixed" wx:elif="{{ tab == 30 }}"><image mode="widthFix" src="../../assets/img/ico-end.png"></image></view>
      </view>
    </view>

    <view class="list list_no tac" wx:if="{{ !list.length && !loading }}">
      <image mode="widthFix" src="../../assets/img/order_no.png"></image>
      <text class="tips block tac">暂无数据</text>

      <block wx:if="{{ list.length == 0 && !total.usable_money }}">
      <view class="mgt_50 mgl_100 mgr_100"><wux-divider text="与衣橱无忧一起创造旧衣零抛弃计划" /></view>
      <navigator url="../../pages/intro/intro"><image mode="widthFix" src="../../assets/img/order_nopic.jpg" class="bigimg"></image></navigator>
      </block>
    </view>
  </view>

  <view class="nav flex flex_mc">
    <navigator url="../../pages/index/index" open-type="redirect"><text class="iconfont icon-yiwuhuishou-heise"></text>衣物回收</navigator>
    <navigator url="../../pages/user/user" open-type="redirect" class="cur"><text class="iconfont icon-youhuiquan-heise-"></text>我的优惠券</navigator>
  </view>
</view>